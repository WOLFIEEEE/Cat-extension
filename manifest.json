{
  "name": "Cat Extension",
  "version": "1.0",
  "description": "Replaces all images on web pages with a random cat GIF",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
